<?xml version="1.0" encoding="ISO-8859-1"?>
<decorators defaultdir="/WEB-INF/sitemesh-decorators">

	<excludes>
		<!-- Use following pattern to exclude main.jsp decoration. Also here URL patterns are request URLs -->
	    
	    <pattern>/flexweb/*</pattern>
	    <pattern>/undecorated/*</pattern>
	    <pattern>/test.html</pattern>
	</excludes>
	
    <decorator name="main" page="main.jsp">
        <pattern>/*</pattern>
    </decorator>
    
    
</decorators>